<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.store.mapper.ShoppingCartMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.com.store.model.ShoppingCart">
        <result column="good_id" property="goodId" />
        <result column="good_num" property="goodNum" />
        <result column="good_name" property="goodName" />
        <result column="user_id" property="userId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        good_id AS goodId, good_num AS goodNum, good_name AS goodName, user_id AS userId
    </sql>

</mapper>
